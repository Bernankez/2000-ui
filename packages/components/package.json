{
  "name": "2000-ui",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.3",
  "description": "A Vue 3 component library, presenting early-2000s web design.",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/2000-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/2000-ui.git"
  },
  "bugs": "https://github.com/Bernankez/2000-ui/issues",
  "keywords": [
    "2000-ui",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./es/index.mjs"
    },
    "./style": "./es/styles/index.css"
  },
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./es/*",
        "./es/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "es",
    "lib",
    "volar.d.ts",
    "web-types.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && pnpm build:esm_cjs && pnpm build:umd",
    "build:esm_cjs": "vite build",
    "build:umd": "vite build --config vite.umd.ts"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@2000-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "unocss": "^0.53.4",
    "vite-plugin-dts": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
